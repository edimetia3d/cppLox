class GG{
    init(ggi) {
        this.ggi = ggi;
    }
    printme(){
        print this.ggi;
        fun closure(){
            print "closure with this";
            print this.ggi;
            print "closure with this";
        }
        return closure;
    }
}

class Bar < GG{
    init(gi) {
        GG.init(gi+1);
        this.gi = gi;
    }
    printme(){
        print this.gi;
        return GG.printme();
    }
}

class Foo < Bar {
  init(radius) {
